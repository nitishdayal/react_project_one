{
  "name": "react_babel_webpack",
  "version": "1.0.0",
  "description": "First attempt at react + babel + webpack starter",
  "main": "index.js",
  "author": "Nitish Dayal",
  "license": "MIT",
  "scripts": {
    "start:dev": "webpack-dashboard -- webpack-dev-server --env=dev --open",
    "start:prod": "webpack-dashboard -- webpack-dev-server --env=prod --open",
    "build:dev": "rm -rf dist && webpack-dashboard -- webpack --env=dev",
    "build:prod": "rm -rf dist && webpack-dashboard -- webpack --env=prod"
  },
  "engines": {
    "npm": ">=3.0.0"
  },
  "engineStrict": true,
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.13.1",
    "eslint-config-formidable": "^2.0.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "html-webpack-plugin": "^2.26.0",
    "webpack": "^2.2.0-rc.4",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.1"
  }
}
